{
  "kind": "collectionType",
  "collectionName": "ambassadors",
  "info": {
    "singularName": "ambassador",
    "pluralName": "ambassadors",
    "displayName": "Ambassador",
    "description": "Grant ambassadors with profiles and social links"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "maxLength": 255,
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": ["images"]
    },
    "linkedinUrl": {
      "type": "string",
      "maxLength": 500
    },
    "grantReportUrl": {
      "type": "string",
      "maxLength": 500
    }
  }
}
